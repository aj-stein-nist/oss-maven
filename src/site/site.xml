	<publishDate position="bottom" format="yyyy-MM-dd" />
	<version position="left" />

	<skin>
		<groupId>lt.velykis.maven.skins</groupId>
		<artifactId>reflow-maven-skin</artifactId>
		<version>1.1.1</version>
	</skin>
	<custom>
		<reflowSkin>
			<theme>bootswatch-cosmo</theme>
			<highlightJs>true</highlightJs>
			<skinAttribution>false</skinAttribution>
			<brand>
				<name>
	        <![CDATA[
	        <img src="images/nist_logo.png" alt="National Institute of
            Standards and Technology" title="Go to nist.gov">
	        ]]>
				</name>
				<href>http://www.nist.gov/</href>
			</brand>
			<titleTemplate>%2$s | %1$s</titleTemplate>
			<toc>top</toc>
			<topNav>Download|Modules|NIST</topNav>
			<bottomNav maxSpan="9">
				<column>Main|modules|Download</column>
				<column>reports</column>
			</bottomNav>
			<bottomDescription>
			<![CDATA[
				<div><span class="color-highlight">oss-maven</span> provides default Maven build
				settings for a number of NIST projects. This allows the site to be delpoyed when the
				code is also deployed, and ensures that basic development requirements are met. This
				includes running PMD to identify
				any serious coding issues, code style checking, and verification that the NIST
				license notice has been included in all source files. Code coverage and complexity
				reports are also generated as part of the site production.</div>
		    ]]>
		    </bottomDescription>
			<pages>
				<index project="oss-maven">
					<shortTitle>Welcome</shortTitle>
					<breadcrumbs>false</breadcrumbs>
					<toc>false</toc>
					<sections>
						<body />
					</sections>
				</index>
				<index>
					<!-- no ToC on the main index page -->
					<toc>false</toc>
					<markPageHeader>false</markPageHeader>
					<sections>
						<body />
					</sections>
				</index>
				<contribute>
					<breadcrumbs>false</breadcrumbs>
				</contribute>
				<!-- Disable source highlighting for Maven reports -->
				<source-repository>
					<highlightJs>false</highlightJs>
				</source-repository>
				<issue-tracking>
					<highlightJs>false</highlightJs>
					<toc>false</toc>
				</issue-tracking>
				<license>
					<highlightJs>false</highlightJs>
					<toc>false</toc>
				</license>
				<!-- Disable ToC for some Maven reports -->
				<project-info>
					<toc>false</toc>
				</project-info>
				<github-report>
					<toc>false</toc>
				</github-report>
				<dependencies>
					<tocTopMax>4</tocTopMax>
				</dependencies>
			</pages>
		</reflowSkin>
	</custom>
	<body>

		<links>
			<!-- <item name="Download" href="oss-parent/index.html#Usage" /> -->
			<item name="GitHub project" href="http://github.com/usnistgov/oss-maven" />
